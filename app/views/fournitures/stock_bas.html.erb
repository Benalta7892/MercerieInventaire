<h1>Fournitures#stock_bas</h1>
<p>Find me in app/views/fournitures/stock_bas.html.erb</p>

<ul>
  <% @fournitures.each do |f| %>
    <li>
      <%= link_to f.name, fourniture_path(f) %> — <%= f.quantity %> <%= f.unit %>
      <%= button_to "Ajouter à la liste d'achat", liste_achat_items_path(fourniture_id: f.id), method: :post %>
    </li>
  <% end %>
</ul>

<p><%= link_to "Retour", fournitures_path %></p>
